{
	"name": "quick_downloader",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"abort-controller": "^2.0.2",
		"bad-words": "^3.0.1",
		"electron": "latest",
		"mousetrap": "^1.6.3",
		"node": "^11.13.0",
		"pac-resolver": "git+https://git@github.com/J-Cake/Node-PAC",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-scripts": "^2.1.8",
		"valid-filename": "^3.1.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "node pre-build.js && react-scripts build && node post-build.js",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"electron": "electron .",
		"pack-osx": "npm run build && electron-packager . --overwrite --platform=darwin --arch=x64  --icon=./src/res/logo_mac.icns  --prune=true --out=release-builds",
		"pack-win": "npm run build && electron-packager --platform=win32 --arch=ia32 --overwrite --icon=./build/favicon.ico  --out=release-builds",
		"run-build": "electron ./build"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"description": "We Download Things Quickly!",
	"main": "./main.js",
	"devDependencies": {
		"electron": "latest",
		"electron-packager": "latest"
	},
	"productName": "Quick Downloader",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jbis9051/quick_download.git"
	},
	"keywords": [
		"Quick",
		"Download"
	],
	"author": "JBis, J-Cake",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jbis9051/quick_download/issues"
	},
	"homepage": "./",
	"build": {
		"appId": "com.quick-downloader",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "assets"
		}
	},
	"eslintConfig": {
		"extends": "react-app",
		"env": {
			"es6": true,
			"browser": true,
			"node": true
		}
	}
}
